    "components": {
        "schemas": {
            "NativeXml.Contracts.V1.EntityContractV1": {
                "required": [
                    "name",
                    "difftest"
                ],
                "type": "object",
                "properties": {
                    "blob": {
                        "type": "string",
                        "description": "Gets or sets an arbitrary BLOB for the client to associate data with the entity."
                    },
                    "count": {
                        "type": "integer",
                        "description": "Gets or sets the count associated with this entity.",
                        "format": "int32"
                    },
                    "id": {
                        "type": "string",
                        "description": "Gets or sets the unique identifier for the entity."
                    },
                    "legacy": {
                        "type": "boolean",
                        "description": "Gets or sets the legacy status."
                    },
                    "name": {
                        "type": "string",
                        "description": "Gets or sets the name of the entity."
                    },
                    "difftest": {
                        "type": "string",
                        "description": "Gets or sets the DiffTest."
                    }
                }
            },
            "NativeXml.Contracts.V1.PagedEntityContractV1": {
                "required": [
                    "pagingInfo"
                ],
                "type": "object",
                "properties": {
                    "items": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/NativeXml.Contracts.V1.EntityContractV1"
                        },
                        "description": "Gets the items in this page of the entities.",
                        "readOnly": true
                    },
                    "pagingInfo": {
                        "$ref": "#/components/schemas/NativeXml.Contracts.V1.PagingInfoContractV1"
                    }
                }
            },
            "NativeXml.Contracts.V1.PagingInfoContractV1": {
                "type": "object",
                "properties": {
                    "continuationToken": {
                        "type": "string",
                        "description": "Gets or sets the continuationToken of this collection."
                    },
                    "totalItems": {
                        "type": "integer",
                        "description": "Gets or sets the total count of the items in this collection.",
                        "format": "int32"
                    }
                }
            }
        }
